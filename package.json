{
  "name": "resumemakr",
  "private": true,
  "description": "Resumemakr is platform for building career propelling resumes",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:staging": "env-cmd -e staging yarn build",
    "build:production": "rm -rf .cache public && env-cmd -e production yarn build",
    "develop": "env-cmd -e dev gatsby develop --port 3023",
    "start:d": "yarn develop",
    "start:e": "env-cmd -e e2e gatsby develop --port 3023",
    "start:staging": "yarn gatsby-serve",
    "start-build-staging": "yarn build:staging && yarn start:staging",
    "gatsby-serve": "gatsby serve --port 3022",
    "gatsby-build-serve": "yarn build:staging && yarn gatsby-serve",
    "test": "env-cmd -e test jest --runInBand",
    "test:w": "yarn test --watch",
    "test:c": "rm -rf coverage && yarn test --coverage",
    "cy-run": "start-server-and-test start-build-staging :3022 cy-run-staging",
    "cy-open": "env-cmd -e e2e cypress open",
    "cy-run-dev": "cypress run --config baseUrl=http://localhost:3023/",
    "type-check": "tsc --noEmit",
    "type-check-cypress": "yarn tsc --noEmit --project ./cypress",
    "gen-apollo-types": "rimraf src/graphql/apollo-types && apollo codegen:generate --endpoint=http://localhost:4025/api --tagName=gql --target=typescript --includes=src/graphql/apollo/*.ts --outputFlat=src/graphql/apollo-types",
    "gen-gatsby-types": "rm -rf src/graphql/gatsby-types && apollo codegen:generate --endpoint=http://localhost:3023/___graphql --tagName=graphql --target=typescript --includes=src/**/*.ts* --outputFlat=src/graphql/gatsby-types",
    "build-deploy": "yarn build:production && yarn deploy",
    "deploy": "netlify deploy --prod --dir=public"
  },
  "dependencies": {
    "@absinthe/socket": "^0.2.1",
    "@absinthe/socket-apollo-link": "^0.2.1",
    "@apollo/react-hooks": "^3.1.3",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-cache-persist": "0.1.1",
    "apollo-client": "2.6.4",
    "apollo-link": "1.2.13",
    "apollo-link-error": "1.1.12",
    "apollo-link-http": "1.5.16",
    "classnames": "^2.2.6",
    "formik": "1.5.4",
    "gatsby": "2.15.15",
    "graphql": "14.5.5",
    "graphql-tag": "2.10.1",
    "immer": "4.0.0",
    "immutability-helper": "^3.0.0",
    "lodash": "4.17.13",
    "phoenix": "1.4.3",
    "react": "16.9.0",
    "react-apollo": "3.1.3",
    "react-dom": "16.9.0",
    "react-helmet-async": "^1.0.2",
    "react-loadable": "^5.5.0",
    "react-transition-group": "^2.7.1",
    "semantic-ui-less": "^2.4.1",
    "semantic-ui-react": "0.88.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.3.3",
    "@cypress/webpack-preprocessor": "4.0.3",
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.1.4",
    "@types/classnames": "^2.2.9",
    "@types/graphql": "^14.0.7",
    "@types/jest": "24.0.12",
    "@types/lodash": "^4.14.123",
    "@types/node": "12.0.0",
    "@types/phoenix": "1.4.0",
    "@types/reach__router": "1.2.4",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-helmet": "5.0.8",
    "@types/react-loadable": "^5.5.2",
    "@types/react-transition-group": "2.9.2",
    "@types/yup": "0.26.13",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "apollo": "2.11.0",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-const-enum": "^0.0.4",
    "babel-preset-gatsby": "0.1.11",
    "babel-preset-react-app": "9.0.0",
    "cypress": "3.4.1",
    "cypress-file-upload": "^3.0.9",
    "cypress-testing-library": "^2.3.6",
    "env-cmd": "9.0.1",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-graphql": "3.0.3",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "gatsby-image": "2.2.19",
