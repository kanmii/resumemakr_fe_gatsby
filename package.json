    "gatsby-plugin-env-variables": "1.0.1",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-less": "3.0.6",
    "gatsby-plugin-manifest": "2.2.16",
    "gatsby-plugin-netlify": "2.1.13",
    "gatsby-plugin-offline": "3.0.7",
    "gatsby-plugin-react-helmet": "3.1.7",
    "gatsby-plugin-sass": "2.1.14",
    "gatsby-plugin-sharp": "2.2.22",
    "gatsby-plugin-typescript": "2.1.7",
    "gatsby-source-filesystem": "2.1.22",
    "gatsby-transformer-sharp": "2.2.14",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-dom": "^3.1.3",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.3.1",
    "less": "3.10.3",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "react-app-polyfill": "1.0.1",
    "rimraf": "2.6.3",
    "serve": "11.0.0",
    "start-server-and-test": "1.10.2",
    "typescript": "3.6.3",
    "workbox-window": "4.3.1"
  },
  "resolutions": {
    "graphql": "14.5.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0"
  }
}
